<html>
  <head>
    <script src="./aframe2/dist/aframe-v0.8.0.min.js"></script>
    <script src="./ar2/aframe/build/aframe-ar.js"></script>
  </head>
  <body>
    <a-scene embedded arjs ='trackingMethod: best;'>
    <a-anchor hit-testing-enabled='true'>

    <a-entity
      position="0 0.5 0"
      scale="0.005 0.005 0.005"
      gltf-model=”url(prebussola/scene.gltf);”>
      <a-animation
        attribute="position"
        dur="1000"
        from="0 0.5 0"
        to="0 2 0"
        repeat="indefinite"
      ></a-animation>

      </a-entity>
    </a-anchor>
      <a-camera-static/>
    </a-scene>
  </body>
</html>
